<h1> Links </h1>
<ul id='links'>
  <% @links.each do |link| %>
    <li>
      Title: <%= link.title %>
      URL:   <%= link.url   %>
      tags: <%= link.tags.map(&:name) %>
    </li>
  <% end %>
</ul>
<form action="/links/new" method="get">
<input type="submit" name ="submit" value="new link">
</form>
